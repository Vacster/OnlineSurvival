            <kind>1</kind>
            <string>var type_event = async_load[? "type"];

switch(type_event){
    case network_type_connect:
        if(!is_server) break;
        if(client == noone){
            client = async_load[? "socket"];
            clientPlayer = instance_create(940, 500, obj_player);
            him = clientPlayer;
            var buffer = buffer_create(1024, buffer_fixed, 1);
            buffer_write(buffer, buffer_u8, CONNECT);
            buffer_write(buffer, buffer_u16, serverPlayer.x);
            buffer_write(buffer, buffer_u16, serverPlayer.y);
            buffer_write(buffer, buffer_u8, serverPlayer.image_index);
            buffer_write(buffer, buffer_u32, obj_network.zombies);
            network_send_packet(client, buffer, buffer_tell(buffer));
        }
        break;
        
    case network_type_disconnect:
        if(!is_server){
            instance_deactivate_object(obj_network.serverPlayer);
        }else{
            client = noone;
            instance_deactivate_object(obj_network.clientPlayer);
        }
        break;
        
    case network_type_data:
        var buffer = async_load[? "buffer"];
        buffer_seek(buffer, buffer_seek_start, 0);
        scr_handle_packet(buffer);
        break;
}
</string>
